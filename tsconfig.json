{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "noImplicitAny": true,
    "target": "es5",
    "allowJs": false,
    "allowSyntheticDefaultImports": true
  }
}
